{
  "name": "@chakra-components/core",
  "version": "0.1.1",
  "description": "A set of wrapper components around chakra ui",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/index",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --root-mode upward src -d dist --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --root-mode upward src -d dist/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --root-mode upward src -d dist/es --ignore \"**/*.test.js\"",
    "build:umd": "cross-env NODE_ENV=production-umd rollup -c",
    "version": "yarn build",
    "inte-test": "npx wdio wdio.conf.js"
  },
  "dependencies": {
    "@reach/auto-id": "^0.10.1",
    "match-sorter": "^4.1.0",
    "react-window": "^1.8.5"
  },
  "peerDependencies": {
    "@chakra-ui/core": "0.7.0",
    "@emotion/core": "10.x",
    "@emotion/styled": "10.x",
    "emotion-theming": "10.x",
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "author": "Timeless <hello@timeless.co>",
  "license": "MIT",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/timelessco/chakra-components"
  },
  "keywords": [
    "react",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "emotion",
    "library"
  ],
  "devDependencies": {
    "@wdio/cli": "^6.1.12",
    "@wdio/local-runner": "^6.1.12",
    "@wdio/mocha-framework": "^6.1.8",
    "@wdio/spec-reporter": "^6.1.12",
    "@wdio/sync": "^6.1.8",
    "chromedriver": "^83.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "wdio-chromedriver-service": "^6.0.3"
  }
}
